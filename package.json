{
  "name": "funkynode",
  "version": "1.0.0",
  "description": "Sample showing a method of working with TypeScript based Azure Functions",
  "main": "index.js",
  "scripts": {
    "prebuild": "typings install",
    "build": "tsc",
    "prewatch": "typings install",
    "watch": "tsc -w --project ./src/tsconfig.json",
    "run": "nodemon ./lib/app.js --delay .5",
    "watchalarmbotsample": "tsc -w --project ./samples/alarmbot-ts/tsconfig.json",
    "runalarmbotsample": "nodemon ./output/samples/samples/alarmbot-ts/app.js --delay .5",
    "watchguessbotsample": "tsc -w --project ./samples/numbergame-ts/tsconfig.json",
    "runguessbotsample": "nodemon ./output/samples/samples/numbergame-ts/app.js --delay .5",
    "watchdialogsample": "tsc -w --project ./samples/dialogs-ts/tsconfig.json",
    "rundialogsample": "nodemon ./output/samples/samples/dialogs-ts/app.js --delay .5",
    "test": "tsc --project ./tests/tsconfig.json && ava ./output/test/tests/run/**/*.js",
    "avabuild": "tsc -w --project ./tests/tsconfig.json",
    "avarun": "ava -w ./output/test/tests/run/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakkaj/funkynode.git"
  },
  "keywords": [
    "Azure",
    "Functions",
    "Node",
    "TypeScript"
  ],
  "author": "jakkaj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakkaj/funkynode/issues"
  },
  "homepage": "https://github.com/jakkaj/funkynode#readme",
  "devDependencies": {
    "@types/node": "^8.0.2",
    "@types/restify": "^4.3.5",
    "@types/sinon": "^2.3.2",
    "ava": "^0.19.1",
    "azure-functions-typescript": "github:christopheranderson/azure-functions-typescript",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.18.0",
    "gulp-run": "^1.7.1",
    "gulp-tsc": "^1.3.2",
    "replace": "^0.3.0",
    "sinon": "^2.3.6",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@types/node": "^8.0.17",
    "@types/sinon": "^2.3.3",
    "azure-functions-typescript": "github:christopheranderson/azure-functions-typescript",
    "botbuilder": "^4.0.0-m3.0",
    "botbuilder-dialogs": "^4.0.0-m3.0",
    "inversify": "^4.11.1",
    "moment": "^2.22.0",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.10",
    "restify": "^4.3.2",
    "sinon": "^2.4.1"
  }
}
